{
  "files.exclude": {
    "vendor/sublime-fish/*": true,
    "vendor/sublime-fish/fish.YAML-tmLanguage": false
  },
  "json.schemas": [
    {
      "fileMatch": [
        "*.tmLanguage.json"
      ],
      "url": "./.vscode/schemas/tmLanguage.json"
    }
  ],
  "typescript.tsc.autoDetect": "off"
}
