{
  "#!/usr/bin/env": {
    "prefix": "!env",
    "body": [
      "#!/usr/bin/env ${1:fish}"
    ]
  },
  "begin … end block": {
    "prefix": "begin",
    "body": [
      "begin",
      "\t${0:# commands}",
      "end"
    ]
  },
  "for … in … end block": {
    "prefix": "for",
    "body": [
      "for ${1:varname} in ${2:list}",
      "\t${0:# commands}",
      "end"
    ]
  },
  "function … end block": {
    "prefix": "function",
    "body": [
      "function ${1:funcname}",
      "\t${0:# body}",
      "end"
    ]
  },
  "if … end block": {
    "prefix": "if",
    "body": [
      "if ${1:test}",
      "\t${0:# commands}",
      "end"
    ]
  },
  "switch … case … end block": {
    "prefix": "switch",
    "body": [
      "switch ${1:value}",
      "case ${2:condition}",
      "\t${0:# commands}",
      "end"
    ]
  },
  "while … end block": {
    "prefix": "while",
    "body": [
      "while ${1:test}",
      "\t${0:# commands}",
      "end"
    ]
  }
}
